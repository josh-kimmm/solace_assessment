{
   "name": "db",
   "version": "0.0.1",
   "description": "TypeORM specific scripts for migrations",
   "type": "commonjs",
   "devDependencies": {
      "ts-node": "10.7.0",
      "@types/node": "^16.11.10",
      "typescript": "4.5.2"
   },
   "dependencies": {
      "typeorm": "0.3.17",
      "reflect-metadata": "^0.1.13",
      "pg": "^8.4.0"
   },
   "scripts": {
      "start": "ts-node src/index.ts",
      "typeorm": "typeorm-ts-node-commonjs",
      "sync": "npm run typeorm schema:sync -- -d ./data-source.ts",
      "migration": "npm run typeorm migration:run -- --d ./data-source.ts"
   }
}